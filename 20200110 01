--tc_rep_task  重新导入

DELETE FROM tc_rep_task WHERE Report_Start_Time <=DATE'2019-12-01';

UPDATE tp_gl_exhib_datadict SET key_name= replace(key_name,'融通','测试') WHERE key_name LIKE '%融通%';

UPDATE TC_PF_GRADEDFUND_INFO SET PF_SYNOPSIS = REPLACE(PF_SYNOPSIS,'融通','测试');




